
Public class Conta(){

  private  String nome;
  private  String numeroConta;
  private  Double saldo;


  public Conta(String nome,String numeroConta, Double saldo){
    this.nome = nome;
    this.numeroConta = numeroConta;
    this.saldo = saldo;
  }

  public Conta(){
  }

  public getNome(){
    return this.nome;
  }

  public setNome(String nome){
      this.nome = nome;
  }

  public deposito(double valorDepositado){
    this.saldo = this.saldo + valorDepositado;
  }

  public saque(double valorSacado){
    this.saldo = this.saldo - valorSacado;
  }

}
----------------------------------------------------------------------

@import Conta;


main(){

    Conta contaJoao = New Conta("Joao", "0102", 1000);
    contaJoao.deposito(2000);
    contaJoao.getSaldo(); /// Saldo 1200
    contaJoao.saque(600); /// Saldo  600


}
